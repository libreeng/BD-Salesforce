<?xml version="1.0" encoding="UTF-8"?>
<NamedCredential xmlns="http://soap.sforce.com/2006/04/metadata">
    <allowMergeFieldsInBody>true</allowMergeFieldsInBody>
    <allowMergeFieldsInHeader>true</allowMergeFieldsInHeader>
    <endpoint>https://onsight.librestream.com/oamrestapi/api/launchrequest</endpoint>
    <generateAuthorizationHeader>false</generateAuthorizationHeader>
    <label>Onsight Connect URI</label>
    <!-- 
        After sfdx:source:push, manually edit this within the SF Setup UI:
            1) set Identity Type to "Named Principal"
            2) set Authentication Protocol to "Password Authentication"
            3) set Username to anything non-blank (value won't be used but musn't be empty)
            4) set Password to your Onsight API Key
    -->
    <principalType>Anonymous</principalType>
    <protocol>NoAuthentication</protocol>
</NamedCredential>
